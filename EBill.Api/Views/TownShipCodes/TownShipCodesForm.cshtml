@model TravelInsuranceWebSite.Api.ViewModels.TownShipCodesFormViewModel
@{
    ViewBag.Title = "New";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>TownShipCode</h2>

@using (Html.BeginForm("Save", "TownShipCodes"))
{
    @Html.ValidationSummary(true, "Please fix the following errors.")

    <div class="form-group">
        @Html.LabelFor(m => m.townShipCode.CodeEN)
        @Html.TextBoxFor(m => m.townShipCode.CodeEN, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.townShipCode.CodeEN)
    </div>
   
    @Html.HiddenFor(m => m.townShipCode.Id)
    @Html.AntiForgeryToken()
    <button type="submit" class="btn btn-primary">Save</button>
}

